{
    "version": 2,
    "builds": [
      {
        "src": "vercel/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/app/static/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "vercel/wsgi.py"
      }
    ]
  }
  